{
  "name": "printer",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "concurrently -n server,client -c blue,green \"bun run dev:server\" \"bun run dev:client\"",
    "dev:server": "bun run --watch server/index.ts",
    "dev:client": "bunx vite",
    "build": "bunx vite build",
    "start": "bun run server/index.ts"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.3",
    "concurrently": "^9.2.1",
    "vite": "^7.3.1"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "hono": "^4.11.8",
    "react": "^19.2.4",
    "react-dom": "^19.2.4"
  }
}
